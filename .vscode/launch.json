  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "module": "reporting.${fileBasenameNoExtension}",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug: Current test file",
      "type": "debugpy",
      "request": "launch",
      "args": ["${file}"],
      "module": "pytest",
      "console": "integratedTerminal"
    }
  ]
}
